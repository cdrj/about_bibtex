@article{ ISI:000419019400017,
Author = {Li, Qingbo and Guo, Jin and Sun, Chang-Yin and Wu, Yuanyuan},
Title = {{Adaptive synchronisation for a class of output-coupling complex networks
   with output feedback nodes}},
