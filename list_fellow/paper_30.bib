@inproceedings{ ISI:000432015502002,
Author = {Zhou, Tianwei and Zuo, Zhiqiang and Wang, Yijing},
Editor = {{Liu, T and Zhao, Q}},
Title = {{Synchronization of Lurie Systems with Quantized and Triggered Control
   under Limited Transmission Capacity}},
Booktitle = {{PROCEEDINGS OF THE 36TH CHINESE CONTROL CONFERENCE (CCC 2017)}},
Series = {{Chinese Control Conference}},
Year = {{2017}},
Pages = {{7920-7925}},
Note = {{36th Chinese Control Conference (CCC), Dalian, PEOPLES R CHINA, JUL
   26-28, 2017}},
Organization = {{Chinese Acad Sci, Acad Math \& Syst Sci; China Soc Ind \& Appl Math;
   Liaoning Assoc Automat; Shenyang Univ Chem Technol; Liaoning Univ Sci \&
   Technol; Asian Control Assoc; IEEE Control Syst Soc; Inst Control Robot
   \& Syst; Soc Instrument \& Control Engineers; Chinese Assoc Automat,
   Tech Comm Control Theory; Syst Engn Soc China; Dalian Univ Technol}},
Abstract = {{In this paper. quantized and triggered control is utilized to sample
   output error signal for output feedback synchronization of two Lurie
   systems (formed by linear part and nonlinear one constructed only by the
   measurable output) under limited transmission capacity. With the
   characteristics of output error, we come up with practical quantized and
   triggered strategy to sample the output error instead of the traditional
   equal time sampling. Different from the existing trigger method, the
   proposed method fully utilizes the structure of quantizer and it
   combines quantization error and sampling error as a whole. It is
   thoroughly discussed that the potential Zeno behavior would not occur
   under the proposed method. In addition, transmission bit rate is also
   calculated and it can be maintained within an acceptable range. It is
   demonstrated by two chaotic Chua systems in the simulation that our
   method can not only meet required performance. but also save
   transmission resources.}},
ISSN = {{2161-2927}},
ISBN = {{978-9-8815-6393-4}},
Unique-ID = {{ISI:000432015502002}},
}

